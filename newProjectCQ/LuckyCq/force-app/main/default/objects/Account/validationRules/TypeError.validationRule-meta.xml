<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TypeError</fullName>
    <active>false</active>
    <description>if role contain salesforce then don&apos;t show error.</description>
    <errorConditionFormula>AND (
    ISNEW(),
    CONTAINS($UserRole.Name ,&apos;Salesforce&apos;) 
)</errorConditionFormula>
    <errorMessage>You name  contain salesforce.</errorMessage>
</ValidationRule>
